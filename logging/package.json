{
  "name": "logging",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "echo `` > src/assets/config/config.js && ng serve --port 4400",
    "start:local": "echo `` > src/assets/config/config.js && ng serve --port 4400",
    "start:kyma": "npm run copy-config && ng serve --disable-host-check --host console-dev.kyma.local --port 4400",
    "start:kyma:api": "npm run copy-config && ng serve --disable-host-check --host console-dev.kyma.local --port 4400 --env api-local",
    "build": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint",
    "copy-config": "cp ../config.js src/assets/config/config.js"
  },
  "private": true,
