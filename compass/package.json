{
  "name": "compass",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kyma-project/luigi-client": "^0.4.12",
    "@kyma-project/luigi-core": "^0.4.12",
    "@kyma-project/react-components": "0.0.43",
    "apollo-boost": "^0.4.3",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-state": "^0.4.2",
    "fundamental-react": "^0.5.3",
    "fundamental-styles": "^0.1.0",
    "graphql": "^14.3.1",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start-live-server": "live-server --entry-file=src/index.html .",
    "start": "concurrently \"react-scripts start\" \"npm run buildConfig:watch\" \"wait-on http://localhost:3000/ && npm run start-live-server\" ",
    "start:kyma": "echo 'This component is not ready yet to be served together with Console.'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildConfig": "webpack --mode production --config webpack-generateConfig.config",
    "buildConfig:watch": "webpack -d --config webpack-generateConfig.config --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "webpack-cli": "^3.3.5"
  }
}
