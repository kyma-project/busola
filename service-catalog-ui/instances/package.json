{
  "name": "instances-ui",
  "description": "Instances UI",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@kyma-project/generic-documentation": "https://github.com/magicmatatjahu/console/raw/integrate-docs-component-2/components/generic-documentation/kyma-project-generic-documentation-0.1.0.tgz",
    "@kyma-project/luigi-client": "0.6.0",
    "@kyma-project/react-components": "0.0.43",
    "ajv": "^6.0.0",
    "apollo-cache-inmemory": "^1.3.7",
    "apollo-client": "^2.4.4",
    "apollo-client-transform": "^0.1.0",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "apollo-utilities": "^1.0.24",
    "deep-copy": "^1.4.2",
    "deep-equal": "^1.0.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-apollo": "^2.2.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.4",
    "react-modal": "^3.4.1",
    "react-router": "^4.2.0",
    "react-router-dom": "4.2.2",
    "styled-components": "^3.4.10",
    "styled-components-grid": "^2.2.1",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "prettier": "^1.16.4",
    "react-app-rewired": "^2.0.1",
    "react-scripts": "^3.0.1",
    "safe-buffer": "5.1.2",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "start": "echo `` > public/config/config.js && PORT=8001 react-app-rewired start",
    "start:kyma": "npm run copy-config && BROWSER=none HOST=console-dev.kyma.local PORT=8001 react-app-rewired start",
    "start:kyma:api": "npm run copy-config && REACT_APP_LOCAL_API=true BROWSER=none HOST=console-dev.kyma.local PORT=8001 react-app-rewired start",
    "build": "REACT_APP_ENV=production react-app-rewired build",
    "test": "react-app-rewired test --passWithNoTests --env=jsdom",
    "eject": "react-app-rewired eject",
    "copy-config": "cp ../../config.js public/config/config.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint-fix": "prettier --config ../../.prettierrc --write \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "lint-check": "prettier --config ../../.prettierrc --check \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "eslint-check": "eslint -c .eslinterrc ./src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "unsafe-perm": true
  }
}
