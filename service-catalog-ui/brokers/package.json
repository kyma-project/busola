{
  "name": "@kyma-project/brokers-ui",
  "description": "Brokers UI",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "echo `` > public/config/config.js && PORT=8002 ../../node_modules/.bin/react-app-rewired start",
    "start:kyma": ". ../../scripts/load-cluster-config.sh /.. && BROWSER=none HOST=console-dev.kyma.local PORT=8002 ../../node_modules/.bin/react-app-rewired start",
    "start:kyma:api": ". ../../scripts/load-cluster-config.sh /.. && REACT_APP_LOCAL_API=true BROWSER=none HOST=console-dev.kyma.local PORT=8002 ../../node_modules/.bin/react-app-rewired start",
    "build": ". ../../scripts/load-cluster-config.sh /.. &&  REACT_APP_ENV=production ../../node_modules/.bin/react-app-rewired build",
    "test": "../../node_modules/.bin/react-app-rewired test --passWithNoTests --env=jsdom",
    "eject": "../../node_modules/.bin/react-app-rewired eject"
  },
  "dependencies": {
    "styled-components": "^3.4.10",
    "styled-components-grid": "^2.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "unsafe-perm": true
  }
}
