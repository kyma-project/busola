    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "moment": "^2.22.0",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.2.0",
    "react-jsonschema-form": "^1.0.3",
    "react-modal": "^3.4.1",
    "react-moment": "^0.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
