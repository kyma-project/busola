    "@kyma-project/luigi-client": "0.4.9",
    "@kyma-project/react-components": "0.0.43",
    "apollo-cache-inmemory": "^1.3.7",
    "apollo-client": "^2.4.4",
    "apollo-client-transform": "^0.1.0",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "apollo-utilities": "^1.0.24",
    "deep-equal": "^1.0.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "highlight.js": "^9.12.0",
    "js-yaml": "^3.11.0",
    "react": "^16.8.6",
    "react-apollo": "^2.2.4",
    "react-app-rewired": "^2.0.1",
    "react-dom": "^16.8.6",
    "react-highlight": "^0.12.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scrollable-anchor": "^0.6.1",
    "styled-components": "^3.2.3",
    "yaml-loader": "^0.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "acorn": "^6.0.5",
    "mkdirp": "^0.5.1",
    "prettier": "^1.16.4",
    "safe-buffer": "5.1.2",
    "source-map-explorer": "^1.6.0",
    "react-scripts": "^3.0.1"
  },
  "scripts": {
    "start": "echo `` > public/config/config.js && PORT=8003 react-app-rewired start",
    "start:kyma": "npm run copy-config && BROWSER=none HOST=console-dev.kyma.local PORT=8003 react-app-rewired start",
    "start:kyma:api": "npm run copy-config && REACT_APP_LOCAL_API=true BROWSER=none HOST=console-dev.kyma.local PORT=8003 react-app-rewired start",
    "build": "REACT_APP_ENV=production react-app-rewired build",
    "test": "react-app-rewired test --passWithNoTests --env=jsdom",
    "eject": "react-app-rewired eject",
    "copy-config": "cp ../config.js public/config/config.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint-fix": "prettier --config ../.prettierrc --write \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "lint-check": "prettier --config ../.prettierrc --check \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
