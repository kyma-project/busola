{
  "name": "content-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kyma-project/react-components": "0.0.32",
    "apollo-boost": "^0.1.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "highlight.js": "^9.12.0",
    "js-yaml": "^3.11.0",
    "react": "^16.3.1",
    "react-apollo": "^2.0.4",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.3.1",
    "react-highlight": "^0.12.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-scrollable-anchor": "^0.6.1",
    "styled-components": "^3.2.3",
    "webpack": "^3.8.1",
    "yaml-loader": "^0.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "safe-buffer": "5.1.2"
  },
  "scripts": {
    "prepare": "curl --insecure -o ./src/commons/manifest.yaml https://raw.githubusercontent.com/kyma-project/kyma/master/docs/manifest.yaml",
    "start": "echo `` > public/config/config.js && PORT=8002 react-app-rewired start",
    "start:kyma": "npm run copy-config && BROWSER=none HOST=console-dev.kyma.local PORT=8002 react-app-rewired start",
    "start:kyma:api": "npm run copy-config && REACT_APP_LOCAL_API=true BROWSER=none HOST=console-dev.kyma.local PORT=8002 react-app-rewired start",
    "build": "REACT_APP_ENV=production react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "copy-config": "cp ../config.js public/config/config.js",
    "analyze": "source-map-explorer build/static/js/main.*"
  }
}
