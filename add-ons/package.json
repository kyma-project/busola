{
  "name": "@kyma-project/addons-ui",
  "description": "Addons Configuration UI for Kyma busola",
  "private": true,
  "scripts": {
    "start": " PORT=8004 ../node_modules/.bin/react-app-rewired start",
    "start:kyma": "BROWSER=none PORT=8004 ../node_modules/.bin/react-app-rewired start",
    "start:kyma:api": "REACT_APP_LOCAL_API=true BROWSER=none PORT=8004 ../node_modules/.bin/react-app-rewired start",
    "build": "INLINE_RUNTIME_CHUNK=false REACT_APP_ENV=production ../node_modules/.bin/react-app-rewired build",
    "test": "../node_modules/.bin/react-app-rewired test --passWithNoTests --env=jsdom",
    "eject": "../node_modules/.bin/react-app-rewired eject",
    "type-check": "tsc --noEmit",
    "build:npx": "echo no-op"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
